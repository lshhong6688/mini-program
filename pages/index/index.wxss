/* pages/index/index.wxss */
page{
  width: 100%;
  height: 100%;
  background: #C5EFFF
}
.container{
  display: flex;
  flex-direction:column;
  align-items: center
}
.string{
  width: 100%;
  height: 4rpx;
  background: white
}
.img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin-top:100rpx
}
button{
  margin-top:100rpx;
  background: #C5EFFF;
  font-size: 40rpx;
  color: #2DB4CB
}
.textName{
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 100rpx;
  color: #0E6DB0
}
.start-container{
  border: 1rpx solid #cdd;
  margin-top:100rpx;
  width: 265rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 10rpx
}
.start{
  font-size: 30rpx;
  line-height: 80rpx;
  color: #0019BA
}

