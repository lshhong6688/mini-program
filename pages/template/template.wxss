/* pages/template/template.wxss */
.tem-container{
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}
.blank{
  width: 100%;
  height: 10rpx;
  background:#2B91AF;
  margin-bottom: 10rpx
}
.list-date{
  padding: 10rpx
}
.list-date image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}
.list-date text{
  font-size: 28rpx;
  margin-left: 10rpx
}
.title{
  font-size: 36rpx;
  font-weight: 700;
  margin: 10rpx
}
.content-img{
  width: 100%;
}
.content{
  font-size: 28rpx;
  text-indent: 28rpx;
  margin-top: 20rpx;
}
.collection_love image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-left: 10rpx
}
.collection_love text{
  font-size: 20rpx;
  margin-left: 10rpx
}